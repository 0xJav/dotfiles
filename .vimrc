syntax on
set number
set ai
set hlsearch
set expandtab
set tabstop=2
set shiftwidth=2
set scrolloff=8
set relativenumber
colorscheme base16-default-dark
set cursorline
hi LineNr ctermfg=red ctermbg=NONE
hi CursorLine ctermbg=NONE
hi CursorLineNr ctermfg=yellow ctermbg=NONE
hi Comment ctermfg=green

ino ' ''<left>
ino " ""<left>
ino ( ()<left>
ino [ []<left>
ino { {}<left>
ino < <><left>
