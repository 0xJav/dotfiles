command=~/.local/src/blocks/$BLOCK_NAME
markup=none


[date_part_1]
interval=1
command=date +%a
color=#5a5a5a
separator_block_width=9
align=center

[date_part_21]
interval=1
command=date +%d
color=#A9A9A9
separator_block_width=9
align=center

[date_part_3]
interval=1
command=date +%b
color=#5a5a5a
separator_block_width=9
align=center

[date_part_4]
interval=1
command=date +%H:%M
color=#A9A9A9
separator_block_width=9
align=left
min_width=500

[ethname]
command=ip -o -4 route show to default | awk '{print $5}'
interval=1
color=#5a5a5a
separator_block_width=9

[iface]
command= ~/.local/src/blocks/iface/iface
interval=1
color=#a9a9a9
